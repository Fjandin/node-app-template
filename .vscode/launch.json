{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch App",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "yarn",
            "runtimeArgs": ["start:dev"],
            "port": 9901,
            "timeout": 30000,
            "outputCapture": "std",
            "console": "integratedTerminal",
            "restart": true,
            "sourceMaps": true
        }
    ]
}