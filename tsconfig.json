{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "declaration": true,
        "outDir": "dist",
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": ["./src/**/*.ts"],
    "exclude": ["./src/**/*.test.ts"]
}
