{
  "name": "node-app-template",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "start:dev": "nodemon --exec babel-node --inspect=0.0.0.0:9901 --extensions=.ts --source-maps=inline ./src/index.ts",
    "build": "rm -rf ./dist && yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir=dist --extensions=.ts --source-maps=inline",
    "eslint": "eslint src --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "koa": "^2.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^26.0.14",
    "@types/koa": "^2.11.0",
    "@types/node": "^12.12.25",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-watch": "^7.0.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.4.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
